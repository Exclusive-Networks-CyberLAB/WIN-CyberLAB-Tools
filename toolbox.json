{
  "display_name": "CyberLAB WIN Tools Quickstart",
  "description": "Runs the local CyberLAB-quickstart.ps1 from the cloned repo.",
  "interactive": true,
  "entry": [
    "powershell",
    "-NoLogo",
    "-NoProfile",
    "-ExecutionPolicy",
    "Bypass",
    "chcp 65001 | Out-Null; [Console]::OutputEncoding=[System.Text.Encoding]::UTF8; $OutputEncoding=[System.Text.Encoding]::UTF8; .\\CyberLAB-quickstart.ps1"
  ]
}
